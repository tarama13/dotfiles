[user]
	name = Rin Tarama
	email = km.uec80@gmail.com
[core]
	editor = vim -c \"set fenc=utf-8\"
	pager = less
	autocrlf = input
	quotepath = false
	excludefile = ~/.gitignore_global
[color]
	ui = true
	diff = auto
	status = auto
	branch = auto
[push]
	default = simple
[alias]
	st = status
	cm = commit
	cmm = commit -m
	co = checkout
	b = branch
	cb = checkout -b
	p = push
	tree = log --graph --all --format=\"%x09%C(cyan bold)%an%Creset%x09%C(yellow)%h%Creset %C(magenta reverse)%d%Creset %s\"
